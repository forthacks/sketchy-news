{
  "name": "dbunk.ai",
  "version": "1.0",
  "description": "Using ML to detect suspicious news articles",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "Open popup"
  },
  "options_page": "options/options.html",
  "manifest_version": 2
}